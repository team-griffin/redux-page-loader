{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7"
          ],
        },
        "modules": false,
        "useBuiltIns": "usage",
      }
    ]
  ],

  "plugins": [
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    "ramda"
  ]
}
